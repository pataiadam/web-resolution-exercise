<script>
  $(document).ready(function(){
    $('#showT').click(function(event) {
      $('#tran').toggle('show');
    });
    $('#showE').click(function(event) {
      $('#exec').toggle('show');
    });
  });
</script>
<div class="row">
  <div class="col-lg-12 text-center">
    <%- task %>

    <p id="tran" style="display: none"> <%- transform %> </p>
    <br><button id="showT" class="btn btn-default">Transform</button><br>

    <p id="exec" style="display: none">     <%- execute %> </p>
    <br><button id="showE" class="btn btn-default">Execute</button>

  </div>
</div>
